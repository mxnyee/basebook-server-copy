{
    "accountType": {
        "type": "string",
        "enum": ["Regular", "Premium", "Deluxe"]
    },
    "username": {
        "type": "string",
        "pattern": "^\\w+$",
        "minLength": 4,
        "maxLength": 64
    },
    "email": {
        "type": "string",
        "format": "email"
    },
    "password": {
        "type": "string",
        "pattern": "^\\w[\\w!@#$%^&*,.?']*$",
        "minLength": 4,
        "maxLength": 64
    },
    "name": {
        "type": "string",
        "maxLength": 64
    },
    "locationName": {
        "type": "string",
        "maxLength": 64
    },
    "city": {
        "type": "string",
        "maxLength": 64
    },
    "state": {
        "type": "string",
        "minLength": 2,
        "maxLength": 2
    },
    "title": {
        "type": "string",
        "maxLength": 64
    },
    "text": {
        "type": "string",
        "maxLength": 1024
    },
    "itemId": {
        "type": "string",
        "pattern": "^\\w*$",
        "minLength": 3,
        "maxLength": 3
    }
}